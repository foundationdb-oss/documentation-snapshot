        "avatar_template": "/user_avatar/forums.foundationdb.org/jugaadi/{size}/477_2.png",
        "created_at": "2019-01-16T02:04:41.463Z",
        "cooked": "<aside class=\"quote no-group\" data-username=\"dave\" data-post=\"3\" data-topic=\"209\">\n<div class=\"title\">\n<div class=\"quote-controls\"></div>\n<img loading=\"lazy\" alt=\"\" width=\"24\" height=\"24\" src=\"https://sea1.discourse-cdn.com/foundationdb/user_avatar/forums.foundationdb.org/dave/48/89_2.png\" class=\"avatar\"> dave:</div>\n<blockquote>\n<p>FoundationDB is written in a memory unsafe language and has not been carefully audited for security bugs</p>\n</blockquote>\n</aside>\n<p>Won\u2019t simulations catch such bugs?</p>",
        "post_number": 5,
        "post_type": 1,
