        "username": "tudor",
        "avatar_template": "/user_avatar/forums.foundationdb.org/tudor/{size}/1253_2.png",
        "created_at": "2022-10-20T21:48:23.495Z",
        "cooked": "<p>I\u2019m trying to use the official FDB docker image for local development and I\u2019d like to set some knobs, how do I do that? <code>docker run foundationdb/foundationdb:... --knob_min_available_space_ratio=0</code> seems to do nothing.</p>",
        "post_number": 1,
        "post_type": 1,
        "posts_count": 2,
        "updated_at": "2022-10-20T21:48:23.495Z",
        "reply_count": 0,
        "reply_to_post_number": null,
        "quote_count": 0,
        "incoming_link_count": 133,
        "reads": 22,
        "readers_count": 21,
        "score": 659.2,
